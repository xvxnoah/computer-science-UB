Cell["\<\
A continuaci\[OAcute] \[OpenCurlyQuote]k\[CloseCurlyQuote] ser\[AGrave] el \
nombre de nombres primers amb qu\[EGrave] es vol fer la comprovaci\[OAcute] i \
\[OpenCurlyQuote]e\[CloseCurlyQuote] ser\[AGrave] fins a quin exponent es far\
\[AGrave].\
\>", "Text",
 CellChangeTimes->{{3.8254387277133923`*^9, 
  3.825438752845149*^9}},ExpressionUUID->"eab2b457-ce2f-44b1-a2c6-\
3f31a44872f3"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"TestPropietat", "[", 
   RowBox[{"k_", ",", " ", "e_"}], "]"}], " ", ":=", " ", 
  RowBox[{
   RowBox[{"Count", "[", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"EulerPhi", "[", 
         RowBox[{
          RowBox[{"Prime", "[", "n", "]"}], "^", "m"}], "]"}], " ", 
        "\[Equal]", " ", 
        RowBox[{
         RowBox[{
          RowBox[{"Prime", "[", "n", "]"}], "^", "m"}], "-", 
         RowBox[{
          RowBox[{"Prime", "[", "n", "]"}], "^", 
          RowBox[{"(", 
           RowBox[{"m", "-", "1"}], ")"}]}]}]}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"n", ",", " ", "k"}], "}"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"m", ",", " ", "e"}], "}"}]}], "]"}], ",", " ", 
     RowBox[{"{", 
      RowBox[{"repeat", "[", 
       RowBox[{"True", ",", " ", "e"}], "]"}], "}"}]}], "]"}], " ", 
   "\[Equal]", " ", "k"}]}], "\[IndentingNewLine]", 
 RowBox[{"TestPropietat", "[", 
  RowBox[{"100", ",", " ", "10"}], "]"}]}], "Input",
