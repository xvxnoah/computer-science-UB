 RowBox[{
  RowBox[{"repeat", "[", 
   RowBox[{"m_", ",", " ", 
    RowBox[{"n_Integer", "?", "Positive"}]}], "]"}], " ", ":=", " ", 
  RowBox[{"Sequence", "@@", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{"m", ",", " ", "n"}], "]"}]}]}]], "Input",
